{
  "name": "@kazukit/enfetch",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./timeout": {
      "import": "./dist/timeout.js",
      "types": "./dist/timeout.d.ts"
    },
    "./retry": {
      "import": "./dist/retry.js",
      "types": "./dist/retry.d.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "test": "vitest run",
    "check:format": "biome format ./src",
    "check:lint": "biome lint ./src",
    "fix:format": "biome format --write ./src",
    "fix:lint": "biome lint --write ./src"
  },
  "author": "kazukit",
  "license": "ISC",
  "description": "Web standard fetch wrapper",
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "packageManager": "pnpm@10.6.5",
  "volta": {
    "node": "22.21.1"
  },
  "engines": {
    "node": ">=20.3.0"
  }
}
